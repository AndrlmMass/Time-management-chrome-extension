{
    "manifest_version": 2,
    "name": "My Extension",
    "description": "Track time spent on websites by setting timers to avoid overusage, as well as cooldown after timer is up.",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  